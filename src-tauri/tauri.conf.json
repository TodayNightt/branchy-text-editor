{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "windows": {
      "allowDowngrades": true,
      "certificateThumbprint": null,
      "digestAlgorithm": null,
      "nsis": null,
      "timestampUrl": null,
      "tsp": false,
      "wix": null,
      "webviewInstallMode": {
        "type": "embedBootstrapper"
      }
    },
    "category": "DeveloperTools",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.png",
      "icons/icon.ico"
    ]
  },
  "productName": "branchy",
  "mainBinaryName": "branchy",
  "version": "0.1.0",
  "identifier": "com.editor.branchy",
  "plugins": {},
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "decorations": false,
        "transparent": true,
        "title": "branchy-text-editor",
        "width": 800,
        "height": 600,
        "useHttpsScheme": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}